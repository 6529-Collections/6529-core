  "private": true,
  "scripts": {
    "build:env-schema": "node scripts/build-env-schema.cjs",
    "predev": "npm run build:env-schema",
    "dev": "node scripts/dev-with-fallback.cjs",
    "compile": "tsc",
    "generate": "rm -rf tmp_gen_outp && node ./node_modules/@openapitools/openapi-generator-cli/main generate -i openapi.yaml -g typescript -o tmp_gen_outp --additional-properties=modelPropertyNaming=snake_case && rm -rf generated && mkdir generated && mv tmp_gen_outp/models generated/models && rm -rf tmp_gen_outp && mkdir generated/http && echo 'export type HttpFile = any;' > generated/http/http.ts && rm generated/models/all.ts",
    "base-build": "cross-env NODE_OPTIONS=--max-old-space-size=7680 next build",
    "build": "npm run lint:quiet && npm run base-build",
    "build:lint": "npm run prebuild && npm run lint && npm run base-build && npm run postbuild",
    "prebuild": "npm run build:env-schema && npm run generate",
    "postbuild": "next-sitemap",
    "start": "next start -p 3001",
    "test": "jest --silent --verbose=false --coverageReporters=none",
    "test-json": "jest --silent --json --outputFile=test-results/jest-results.json ; npm run test-extract-failed-names",
    "test-json-changed": "jest --silent --json --outputFile=test-results/jest-results.json --changedSince=main ; npm run test-extract-failed-names",
    "test-extract-failed-names": "node test-results/list-failed-tests.cjs",
    "test:cov": "CI=1 jest --coverage --watchAll=false --verbose=false 2>&1 | grep -A 10 '^FAIL' || true",
    "test:cov:summary": "CI=1 jest --coverage --watchAll=false --silent --reporters=default --verbose=false 2>&1 | grep -E '(PASS|FAIL|failed|passed)' || true",
    "test:cov:verbose": "CI=1 jest --coverage --watchAll=false",
    "test:cov:changed": "CI=1 jest --coverage --watchAll=false --changedSince=main",
    "improve-coverage": "node scripts/improve-coverage.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "nodemon --watch tests --watch src --ext ts,tsx,js,jsx --exec \"npx playwright test\"",
    "knip": "knip --fix --allow-remove-files",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "lint:quiet": "eslint . --quiet",
    "lint": "eslint .",
    "lint:fix": "npx eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "relative-to-alias-imports": "tsx scripts/relative-to-alias-imports.ts",
    "deadcode:knip": "knip",
    "deadcode:exports": "ts-prune -p tsconfig.ts-prune.json --error",
    "deadcode:deps": "depcheck --ignores=\"eslint,eslint-*,@types/*,jest,ts-jest,playwright,@playwright/test,tailwindcss,postcss,autoprefixer\" --ignore-patterns=\".next,coverage,public,generated,tmp_gen_outp\"",
    "deadcode": "npm run deadcode:knip && npm run deadcode:exports && npm run deadcode:deps"
  },
