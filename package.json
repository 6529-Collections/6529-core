{
  "name": "6529-core",
  "version": "0.0.1",
  "author": {
    "name": "6529",
    "email": "6529@6529.io"
  },
  "description": "6529core APP",
  "private": false,
  "license": "MIT",
  "main": "main/electron-src/index.js",
  "scripts": {
    "pull-web": "git subtree pull --prefix=renderer https://github.com/6529-Collections/6529seize-frontend.git main",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rm -rf dist main renderer/out renderer/.next",
    "clean-win": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run clean && npm run build-electron && electron . --dev",
    "dev-win": "npm run clean-win && npm run build-electron-win && electron . --dev",
    "start": "electron . --start",
    "rebuild-sql": "npx electron-rebuild -f -w better-sqlite3",
    "build-renderer": "next build renderer",
    "build-electron": "tsc -p electron-src && cp -r assets main/electron-src/",
    "build-electron-win": "tsc -p electron-src && xcopy /E /I assets main\\electron-src\\assets\\",
    "build": "npm run clean && npm run build-renderer && npm run build-electron",
    "build-win": "npm run clean-win && npm run build-renderer && npm run build-electron-win",
    "dist-win": "npm run build && electron-builder --win --ia32 --x64 --arm64 --publish never && upload-win-unsigned",
    "upload-win-unsigned": "aws s3 sync ./dist s3://6529bucket/6529-core-app/win/unsigned/ --exclude '*' --include '*.exe' --include '*.blockmap' --include 'latest.yml' --exclude '*/**'",
    "download-win-unsigned": "aws s3 sync s3://6529bucket/6529-core-app/win/unsigned/ ./dist",
    "sign-win": "ts-node sign-win.ts",
    "dist-sign-win": "npm run dist-win && npm run sign-win && npm run publish-only",
    "dist-mac": "npm run build && electron-builder --mac --arm64 --x64 --publish always",
    "dist-linux": "npm run build && electron-builder --linux --x64 --publish always",
    "publish-win": "aws s3 sync ./dist s3://6529bucket/6529-core-app/win/",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/model-viewer": "^3.5.0",
    "@headlessui/react": "^2.0.4",
    "@lexical/react": "^0.16.1",
    "@mojs/core": "^1.7.1",
    "@reduxjs/toolkit": "^2.2.5",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/query-core": "^5.45.0",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@tippyjs/react": "^4.2.6",
    "@walletconnect/modal": "^2.6.2",
    "@walletconnect/sign-client": "^2.14.0",
    "@web3modal/wagmi": "^5.0.11",
    "app-root-path": "^3.1.0",
    "better-sqlite3": "^11.1.2",
    "bootstrap": "^5.3.3",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^5.1.7",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.2.1",
    "ethereum-cryptography": "^2.2.0",
    "ethers": "^6.13.2",
    "framer-motion": "^11.3.9",
    "fs": "^0.0.1-security",
    "get-port-please": "^3.1.2",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lexical": "^0.16.1",
    "next": "^14.2.4",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-scroll": "^1.9.0",
    "react-share": "^5.1.0",
    "react-toastify": "^10.0.5",
    "react-toggle": "^4.1.3",
    "react-use": "^17.5.0",
    "react-use-downloader": "^1.2.8",
    "rehype-external-links": "^3.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "^1.77.6",
    "swiper": "^11.1.4",
    "three": "^0.163.0",
    "uuid": "^10.0.0",
    "viem": "^2.17.11",
    "wagmi": "^2.12.0",
    "web3": "^4.11.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@tanstack/eslint-plugin-query": "^5.43.1",
    "@types/better-sqlite3": "^7.6.10",
    "@types/electron-localshortcut": "^3.1.3",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-scroll": "^1.8.10",
    "@types/react-toggle": "^4.0.5",
    "@types/uuid": "^10.0.0",
    "@walletconnect/types": "^2.14.0",
    "autoprefixer": "^10.4.19",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.38",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  },
  "build": {
    "appId": "com.6529.6529core",
    "productName": "6529 CORE",
    "asar": true,
    "protocols": [
      {
        "name": "6529core",
        "schemes": [
          "core6529"
        ]
      }
    ],
    "files": [
      "main/**/*",
      "next.config.mjs",
      "renderer/out/**/*",
      "renderer/public/**/*"
    ],
    "extraResources": [
      "assets/**/*"
    ],
    "publish": [
      {
        "provider": "s3",
        "bucket": "6529bucket",
        "region": "eu-west-1",
        "path": "6529-core-app/${os}/",
        "acl": null
      }
    ],
    "mac": {
      "identity": null,
      "icon": "assets/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64",
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-mac-${arch}-${version}.${ext}",
      "extendInfo": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.6529.6529core",
            "CFBundleURLSchemes": [
              "core6529"
            ]
          }
        ]
      }
    },
    "win": {
      "publisherName": "6529",
      "icon": "assets/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        },
        {
          "target": "nsis",
          "arch": [
            "arm64"
          ]
        }
      ],
      "artifactName": "${productName}-win-${arch}-${version}.${ext}",
      "signingHashAlgorithms": [
        "sha256"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "6529 CORE"
    },
    "linux": {
      "icon": "assets",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-linux-${arch}-${version}.${ext}"
    }
  }
}
