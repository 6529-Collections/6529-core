{
  "name": "6529-core",
  "version": "0.0.1",
  "description": "6529core APP",
  "private": false,
  "license": "MIT",
  "main": "main/electron-src/index.js",
  "scripts": {
    "clean": "rm -rf dist main renderer/out renderer/.next",
    "dev": "npm run build-electron && electron . --dev",
    "start": "electron . --start",
    "rebuild-sql": "npx electron-rebuild -f -w better-sqlite3",
    "build-renderer": "next build renderer",
    "build-electron": "tsc -p electron-src && cp -r assets main/electron-src/",
    "build": "npm run clean && npm run build-renderer && npm run build-electron",
    "pack-app": "npm run build && electron-builder --dir",
    "dist-win": "npm run build && electron-builder --win --ia32 --x64 --arm64 --publish never",
    "dist-mac": "npm run build && electron-builder --mac --x64 --arm64 --publish never",
    "dist-linux": "npm run build && electron-builder --linux --publish never",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/model-viewer": "^3.5.0",
    "@headlessui/react": "^2.0.4",
    "@lexical/react": "^0.16.1",
    "@metamask/sdk": "^0.27.0",
    "@mojs/core": "^1.7.1",
    "@reduxjs/toolkit": "^2.2.5",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/query-core": "^5.45.0",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@tippyjs/react": "^4.2.6",
    "@walletconnect/modal": "^2.6.2",
    "@walletconnect/sign-client": "^2.14.0",
    "@web3modal/wagmi": "^5.0.7",
    "app-root-path": "^3.1.0",
    "better-sqlite3": "^11.1.2",
    "bootstrap": "^5.3.3",
    "csv-parser": "^3.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^5.1.7",
    "electron-store": "^10.0.0",
    "ethereum-cryptography": "^2.2.0",
    "ethers": "^6.13.1",
    "framer-motion": "^11.3.9",
    "fs": "^0.0.1-security",
    "get-port-please": "^3.1.2",
    "i": "^0.3.7",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lexical": "^0.16.1",
    "lowdb": "^7.0.1",
    "next": "^14.2.4",
    "next-redux-wrapper": "^8.1.0",
    "npm": "^10.8.2",
    "pdfjs-dist": "^4.4.168",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-pdf": "^9.1.0",
    "react-redux": "^9.1.2",
    "react-scroll": "^1.9.0",
    "react-share": "^5.1.0",
    "react-toastify": "^10.0.5",
    "react-toggle": "^4.1.3",
    "react-use": "^17.5.0",
    "react-use-downloader": "^1.2.8",
    "rehype-external-links": "^3.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "^1.77.6",
    "swiper": "^11.1.4",
    "three": "^0.163.0",
    "uuid": "^10.0.0",
    "viem": "^2.17.11",
    "wagmi": "^2.12.0",
    "web3": "^4.11.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/better-sqlite3": "^7.6.10",
    "@types/electron-localshortcut": "^3.1.3",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-scroll": "^1.8.10",
    "@types/react-toggle": "^4.0.5",
    "@types/uuid": "^10.0.0",
    "@walletconnect/types": "^2.14.0",
    "autoprefixer": "^10.4.19",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "file-loader": "^6.2.0",
    "next": "^14.2.3",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  },
  "build": {
    "appId": "com.6529.6529core",
    "productName": "6529 CORE",
    "asar": true,
    "protocols": [
      {
        "name": "6529core",
        "schemes": [
          "core6529"
        ]
      }
    ],
    "files": [
      "main/**/*",
      "next.config.mjs",
      "renderer/out/**/*",
      "renderer/public/**/*"
    ],
    "extraResources": [
      "assets/**/*"
    ],
    "mac": {
      "icon": "assets/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        }
      ],
      "artifactName": "${productName}-mac-${arch}-${version}.${ext}",
      "extendInfo": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.6529.6529core",
            "CFBundleURLSchemes": [
              "core6529"
            ]
          }
        ]
      }
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        },
        {
          "target": "nsis",
          "arch": [
            "arm64"
          ]
        }
      ],
      "artifactName": "${productName}-win-${arch}-${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "6529 CORE"
    },
    "linux": {
      "icon": "assets",
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    }
  }
}
